<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
		"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="ticketinformationMapper">
	 

	 
	<select id="getTicketinformationById" parameterType="int" resultType="com.op.spot.entity.Ticketinformation">
		SELECT

         id ,sid ,peakseasonfare ,offseasorates ,becareful ,Preferentialinformation
	        FROM s_ticketinformation st

	        WHERE 

		       sid= #{id }

	</select>

	 
</mapper>